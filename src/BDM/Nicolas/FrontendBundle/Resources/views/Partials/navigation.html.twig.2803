                                        <p class="layerMenu-title">Marque</p>
                                        <ul class="layerMenu-list">
                                            {% for brand in brands %}
                                                <li class="layerMenu-item"><a
                                                            href="{{ path('_products_by_category_by_brand',{'id_category':category.id,'slug_category':category.slug,'brand':brand.slug}) }}"
                                                            class="layerMenu-link">{% if brand.label %}{{ brand.label }}{% endif %}</a>
