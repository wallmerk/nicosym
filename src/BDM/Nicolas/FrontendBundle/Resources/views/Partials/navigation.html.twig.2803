                                        </ul>
                                    </div>
                                {% endif %}
                                <div class="">
                                    <a class="nicoButton nicoButton-default" style="transition:none;"
                                       href="{{ path('_products_by_category',{'id_category':category.id,'slug_category':category.slug}) }}">
                                        <span class="nicoButton-content">
                                            <i class="nicoButton-icon ns-Icon ns-Icon--chevron"></i>
                                            découvrir tous nos {% if category.label %}{{ category.label }}{% endif %}
                                        </span>
                                    </a>
                                </div>
                            </div><!-- EndlefLayer -->

                            <div class="rightLayer">
                                {% for banner in category.getBanners() %}
                                    <a
                                            href="{% if banner.link %}{{ banner.link }}{% endif %}"
                                            class="ns-Layer-bannerLink"><img
                                                src="{{ asset('uploads/backend/product/category/banner/'~banner.banner) }}"
                                                class="ns-Layer-banner" alt="Petits prix"/></a>
                                {% endfor %}
                            </div>
                        </div> <!-- EndRightLayer -->
                    </div> <!-- End MegaDrop -->
                {% endif %}
            {% endif %}
        </li>
    {% endfor %}
</ul>