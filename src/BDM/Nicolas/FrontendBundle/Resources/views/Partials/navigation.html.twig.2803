    {% for item in data %}
        {% set category = item['category'] %}
        {% set regions = item['regions'] %}
        {% set countries = item['countries'] %}
        {% set colors = item['colors'] %}
        {% set sub_categories = item['sub_categories'] %}
        {% set brands = item['brands'] %}

        <li class="ns-principalNav-item"><a
                    href="{{ path('_products_by_category',{'id_category':category.id,'slug_category':category.slug}) }}"
                    class="ns-principalNav-link">{{ category.label }}</a>
            {% if category.code=='vins' %}
                <div class="megadrop">
                    <div class="container">
                        <div class="leftLayer">

                            {% if  regions|length!=0 %}
                                <div class="layerMenu-bloc">
                                    <p class="layerMenu-title">Par région</p>
                                    <ul class="layerMenu-list">
                                        {% for region in regions %}
                                            <li class="layerMenu-item"><a
                                                        href="{{ path('_products_by_category_by_region',{'id_category':category.id,'slug_category':category.slug,'region':region.slug}) }}"
                                                        class="layerMenu-link">{% if region.label %}{{ region.label }}{% endif %}</a>
                                            </li>


                                        {% endfor %}
                                    </ul>
                                </div>
                            {% endif %}
                            {% if countries|length!=0 %}
                                <div class="layerMenu-bloc">
                                    <p class="layerMenu-title">Vins du monde</p>
                                    <ul class="layerMenu-list">
                                        {% for country in countries %}
                                            <li class="layerMenu-item"><a
                                                        href="{{ path('_products_by_category_by_country',{'id_category':category.id,'slug_category':category.slug,'country':country.slug}) }}"
                                                        class="layerMenu-link">{% if country.label %}{{ country.label }}{% endif %}</a>
                                            </li>
                                        {% endfor %}
                                    </ul>
                                </div>
                            {% endif %}
                            {% if colors|length!=0 %}
                                <div class="layerMenu-bloc">
                                    <p class="layerMenu-title">Par Couleur</p>
                                    <ul class="layerMenu-list">
                                        {% for color in colors %}
                                            <li class="layerMenu-item"><a
                                                        href="{{ path('_products_by_category_by_color',{'id_category':category.id,'slug_category':category.slug,'color':color.slug}) }}"
                                                        class="layerMenu-link">{% if color.label %}{{ color.label }}{% endif %}</a>
                                            </li>
                                        {% endfor %}
                                        {#<li class="layerMenu-item"><a href="#" class="layerMenu-link">Rosé</a></li>#}
                                        {#<li class="layerMenu-item"><a href="#" class="layerMenu-link">Blanc</a></li>#}
                                    </ul>
                                    {#<p class="layerMenu-title">Gammes nicolas</p>#}
                                    {#<ul class="layerMenu-list">#}
                                    {#<li class="layerMenu-item"><a href="#" class="layerMenu-link">Les grumes</a></li>#}
                                    {#<li class="layerMenu-item"><a href="#" class="layerMenu-link">les petites Récoltes</a></li>#}
                                    {#<li class="layerMenu-item"><a href="#" class="layerMenu-link">Cub Collection</a></li>#}
                                    {#<li class="layerMenu-item"><a href="#" class="layerMenu-link">Etienne Nicolas</a></li>#}
                                    {#<li class="layerMenu-item"><a href="#" class="layerMenu-link">AOC</a></li>#}
                                    {#<li class="layerMenu-item"><a href="#" class="layerMenu-link">Vins Bio</a></li>#}
                                    {#</ul>#}
                                    {#<p class="layerMenu-title">Vins en primeur</p>#}
                                    {#<ul class="layerMenu-list">#}
