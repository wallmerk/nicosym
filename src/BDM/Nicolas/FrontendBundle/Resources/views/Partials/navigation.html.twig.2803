                                            {#<li class="layerMenu-item"><a href="#" class="layerMenu-link">Moet &amp;#}
                                            {#Chandon</a></li>#}
                                        </ul>
                                    </div>
                                {% endif %}
                                {% if colors|length!=0 %}
                                    <div class="layerMenu-bloc">
                                        <p class="layerMenu-title">Par Couleur</p>
                                        <ul class="layerMenu-list">
                                            {% for color in colors %}
                                                <li class="layerMenu-item"><a
                                                            href="{{ path('_products_by_category_by_color',{'id_category':category.id,'slug_category':category.slug,'color':color.slug}) }}"
                                                            class="layerMenu-link">{% if color.label %}{{ color.label }}{% endif %}</a>
                                                </li>
                                            {% endfor %}
