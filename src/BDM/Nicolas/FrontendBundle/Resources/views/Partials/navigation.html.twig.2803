                                            </li>


                                        {% endfor %}
                                    </ul>
                                </div>
                            {% endif %}
                            {% set cepages = item['cepages'] %}

                            {% if cepages|length!=0 %}
                                <div class="layerMenu-bloc">
                                    <p class="layerMenu-title">Cépages</p>
                                    <ul class="layerMenu-list">
                                        {% for  cepage in cepages %}
                                            <li class="layerMenu-item"><a
                                                        href="{{ path('_products_by_cepage_by_category',{'id_category':category.id, 'slug_category':category.slug, 'cepage':cepage.cepage|replace({' ':'_', '.':''})}) }}"
                                                        class="layerMenu-link">{% if cepage.cepage %}{{ cepage.cepage }}{% endif %}</a>
                                            </li>
                                        {% endfor %}

                                    </ul>
                                </div>
                            {% endif %}
                            <div class="nico-btn-1"><a class="nicoButton nicoButton-default" style="transition:none;"
                                                       href="{{ path('_products_by_category',{'id_category':category.id,'slug_category':category.slug}) }}">
                                    <span class="nicoButton-content">
                                            <i class="nicoButton-icon ns-Icon ns-Icon--chevron"></i>
                                        découvrir tous nos champagnes</span></a></div>
                        </div>

                        <div class="rightLayer">
                            {% for banner in category.getBanners() %}
                                <a
                                        href="{% if banner.link %}{{ banner.link }}{% endif %}"
                                        class="ns-Layer-bannerLink"><img
                                            src="{{ asset('uploads/backend/product/category/banner/'~banner.banner) }}"
                                            class="ns-Layer-banner" alt="Petits prix"/></a>
                            {% endfor %}
                        </div>


                    </div>
                </div>
